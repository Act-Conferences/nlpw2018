<h3>CMS._list as hash</h3>
[% SET CMS = {
    list = [ "hello", "World", "are we here" ]
    other = "John"
} %]
<p>
And now: [% CMS.other %]
</p>
[% PROCESS "cms/$category/index.tt" %]
[% FOREACH item_name IN CMS._list %]
[% item_path = 'cms/' _ category _ '/' _ item_name _ '.tt' %]
[% PROCESS "$item_path" %]
[% END %]
